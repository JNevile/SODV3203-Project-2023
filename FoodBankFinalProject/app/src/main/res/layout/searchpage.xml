<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#035506">

    <Button
        android:id="@+id/logoutbtn2"
        android:layout_width="104dp"
        android:layout_height="35dp"
        android:text="Logout"
        tools:layout_editor_absoluteX="228dp"
        tools:layout_editor_absoluteY="666dp" />

    <Button
        android:id="@+id/homebtn"
        android:layout_width="104dp"
        android:layout_height="35dp"
        android:text="Home"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="666dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="79dp"
        android:layout_height="41dp"
        android:layout_marginBottom="640dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.951"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/brand"
        app:layout_constraintVertical_bias="0.031"
        app:srcCompat="@drawable/handshake_icon_svg" />

    <TextView
        android:id="@+id/brand3"
        android:layout_width="109dp"
        android:layout_height="32dp"
        android:fontFamily="sans-serif-black"
        android:text="FOOD SHARE"
        android:textColor="#99F69C"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.99"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="55dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/carrot_emoji_by_twitter"
        tools:layout_editor_absoluteX="270dp"
        tools:layout_editor_absoluteY="105dp" />

    <TextView
        android:id="@+id/foodfinder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="FOOD FINDER"
        android:textColor="#99F69C"
        android:textSize="34sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="110dp" />
</androidx.constraintlayout.widget.ConstraintLayout>